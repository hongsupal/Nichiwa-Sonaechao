<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:id="@+id/Relative"
    tools:context="com.example.yamashitamasaki.hello_world.ButtonFragment">

    <ImageButton
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="設定"
        android:src="@drawable/setting"
        android:id="@+id/settingbutton"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/bichiku" />

    <ImageButton
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="ホーム"
        android:id="@+id/home"
        android:src="@drawable/home"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="非常食"
        android:src="@drawable/food"
        android:id="@+id/hijousyoku"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/home" />

    <ImageButton
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="備蓄品"
        android:src="@drawable/stock"
        android:id="@+id/bichiku"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/hijousyoku" />

</RelativeLayout>
